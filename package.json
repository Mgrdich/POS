{
  "name": "POS",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Mgrdich/POS.git",
  "author": "Mgo <mgotm13@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "env $(grep  -v '#' .env | tr '\\n' ' ')  node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:dev": "env $(grep  -v '#' .env.dev | tr '\\n' ' ') node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:watch": "env $(grep  -v '#' .env.dev | tr '\\n' ' ') nodemon",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.2",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mongodb": "^3.3.15",
    "@types/mongoose": "^5.7.0",
    "@types/node": "^13.5.2",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/socket.io": "^2.1.4",
    "nodemon": "^2.0.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "date-fns": "^2.12.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.10",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "socket.io": "^2.3.0",
    "ts-node": "^8.6.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
